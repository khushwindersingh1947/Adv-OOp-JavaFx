            <Label stylesheets="@styles.css" text="Our Favorite Songs" BorderPane.alignment="CENTER" />
         </top>
         <bottom>
            <Button mnemonicParsing="false" onAction="#createSongScene" text="Create a Song" BorderPane.alignment="CENTER" />
         </bottom>
         <center>
            <TableView fx:id="tableView" prefHeight="394.0" prefWidth="565.0" AnchorPane.topAnchor="0.0" BorderPane.alignment="CENTER">
              <columns>
                <TableColumn fx:id="songID" prefWidth="75.0" text="SongID" />
                <TableColumn fx:id="name" prefWidth="75.0" text="Name" />
                  <TableColumn fx:id="genre" prefWidth="75.0" text="Genre" />
                  <TableColumn fx:id="length" prefWidth="78.39996337890625" text="Length" />
                  <TableColumn fx:id="artist" prefWidth="78.39996337890625" text="Artist" />
